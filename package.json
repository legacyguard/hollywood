{
  "name": "document-safe-monorepo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "web",
    "mobile",
    "packages/*"
  ],
  "scripts": {
    "dev": "echo 'Use npm run web:dev or npm run mobile:dev'",
    "build": "npm run build:clean && npm run build:all",
    "build:all": "turbo run build",
    "build:packages": "turbo run build --filter='./packages/*'",
    "build:web": "turbo run build --filter=legacyguard-web",
    "build:mobile": "turbo run build --filter=mobile",
    "build:clean": "npm run clean:cache && npm run clean:dist",
    "clean:cache": "rm -rf .turbo mobile/.expo /tmp/metro-*",
    "clean:dist": "rm -rf web/dist mobile/dist packages/*/dist",
    "clean:all": "npm run clean:cache && npm run clean:dist && rm -rf node_modules package-lock.json web/node_modules mobile/node_modules",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "web:dev": "cd web && npm run dev",
    "mobile:dev": "cd mobile && npm run start",
    "web:build": "npm run build:packages && cd web && npm run build",
    "mobile:build": "cd mobile && npx expo export --platform all",
    "test:web": "cd web && npm run test",
    "test:mobile": "cd mobile && npm run test",
    "i18n:check": "tsx scripts/i18n-optimizer.ts analyze",
    "i18n:optimize": "tsx scripts/i18n-optimizer.ts dry-run",
    "i18n:split": "tsx scripts/i18n-optimizer.ts split",
    "i18n:extract": "tsx scripts/extract-hardcoded-texts.ts",
    "preinstall": "bash -lc '[[ $(uname) == Darwin ]] && ./scripts/increase-file-limits.sh || true'",
    "postinstall": "echo 'Dependencies installed successfully!'"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@babel/types": "^7.28.2",
    "@clerk/testing": "^1.12.1",
    "@eslint/js": "^9.34.0",
