{
  "name": "legacyguard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
    "typecheck": "tsc -b --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:ocr": "node scripts/test-ocr.cjs",
    "test:e2e": "cross-env NODE_ENV=test playwright test",
    "test:e2e:ui": "cross-env NODE_ENV=test playwright test --ui",
    "test:e2e:debug": "cross-env NODE_ENV=test playwright test --debug",
    "test:e2e:headed": "cross-env NODE_ENV=test playwright test --headed",
    "test:journey": "cross-env NODE_ENV=test playwright test --config=playwright.journey.config.ts",
    "test:journey:ui": "cross-env NODE_ENV=test playwright test --config=playwright.journey.config.ts --ui",
    "test:journey:debug": "cross-env NODE_ENV=test playwright test --config=playwright.journey.config.ts --debug",
    "test:journey:report": "playwright show-report playwright-report/journey",
    "check:all": "npm run typecheck && npm run lint && npm run format:check && npm run test:unit"
  },
